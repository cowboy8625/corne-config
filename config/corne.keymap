	combos {
		compatible = "zmk,combos";
		combo_backslash {
		    timeout-ms = <COMBO_DELAY>;
		    key-positions = <8 9>; // o and p
		    bindings = <&kp BSLH>;
		    layers = <DEFAULT>;
		};
		combo_backspace {
		    timeout-ms = <COMBO_DELAY>;
		    key-positions = <11 12>; // s and d
		    bindings = <&kp BSPC>;
		    layers = <DEFAULT>;
		};
		combo_ecs {
		    timeout-ms = <COMBO_DELAY>;
		    key-positions = <1 2>;  // w and e
		    bindings = <&kp ESC>;
		    layers = <DEFAULT>;
		};
		combo_less_than {
		    timeout-ms = <COMBO_DELAY>;
		    key-positions = <15 16>;  // h and j
		    bindings = <&kp LT>;
		    layers = <DEFAULT>;
		};
		combo_colon {
		    timeout-ms = <COMBO_DELAY>;
		    key-positions = <16 17>;  // j and k
		    bindings = <&kp COLON>;
		    layers = <DEFAULT>;
		};
		combo_greater_than {
		    timeout-ms = <COMBO_DELAY>;
		    key-positions = <17 18>;  // k and l
		    bindings = <&kp GT>;
		    layers = <DEFAULT>;
		};
		combo_underscore {
		    timeout-ms = <COMBO_DELAY>;
		    key-positions = <26 27>;  // m and ,
		    bindings = <&kp UNDERSCORE>;
		    layers = <DEFAULT>;
		};
		combo_gamepad_layer {
		    timeout-ms = <COMBO_DELAY>;
		    key-positions = <5 6 7 8 9>;  // yuiop
		    bindings = <&to GAMEPAD>;
		    layers = <DEFAULT GAMEPAD>;
		};
		combo_blue_tooth_layer {
		    timeout-ms = <COMBO_DELAY>;
		    key-positions = <15 16 17 18 19>;  // hjkl;
		    bindings = <&to BLUE_TOOTH_LAYER>;
		    layers = <DEFAULT BLUE_TOOTH_LAYER>;
		};
	};

